#include<stdio.h>
int main()
{
    int n;
    printf("enter the value of n");
    scanf("%d",&n);
    if(n%3==0)
    {
        if(n%5==0)
        {
            printf("the given numer is divisible by 3 and 5");
        }
    }
    else{
        printf("the number is not divisible by 3 and 5");
    }
    return 0;
}